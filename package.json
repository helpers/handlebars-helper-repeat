{
  "name": "handlebars-helper-repeat",
  "description": "Handlebars block helper for repeating whatever is inside the block _n_ times.",
  "version": "1.0.0",
  "homepage": "https://github.com/helpers/handlebars-helper-repeat",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "helpers/handlebars-helper-repeat",
  "bugs": {
    "url": "https://github.com/helpers/handlebars-helper-repeat/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "mocha --reporter spec test.js",
    "test:debug": "node  --inspect-brk node_modules/mocha/bin/_mocha test.js"
  },
  "main": "index.js",
  "engines": {
    "node": ">= 0.10.0"
  },
  "devDependencies": {
    "engine-handlebars": "^0.8.0",
    "gulp-format-md": "^0.1.11",
    "handlebars": "^4.0.6",
    "handlebars-helpers": "^0.9.8",
    "mocha": "^3.2.0",
    "templates": "^0.25.3"
  },
  "keywords": [
    "assemble",
    "assemblehelper",
    "handlebars",
    "handlebars helper",
    "helper",
    "helpers",
    "render",
    "repeat",
    "template",
    "templates"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "handlebars-helpers",
        "template-helpers"
      ]
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
